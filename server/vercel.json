{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/api/v1/user/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/api/v1/launch/(.*)",
      "dest": "/dist/index.js"
    }
  ]
}
